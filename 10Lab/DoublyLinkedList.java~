    public E getFirst() {
        if (size == 0) {
        	System.out.println("Queue is Empty!");
        	
        }
        
        return null;  //return the element here 
    }
    
    
     

    public void removeLast() {
        if (size == 0) {
        	System.out.println("Queue is Empty!");
        	
        }
        
       

    }
     
    
    public E getLast() {
        if (size == 0) {
        	System.out.println("Queue is Empty!");
        	
        }
        System.out.println("The Last Element is: " );
        
        return null; //return the last element
    }
    
    
    
    
    
    
    
  
}