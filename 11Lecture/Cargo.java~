import java.util.*;
public class Cargo
{
     private String dest = "";
     private double tons = 0.0;
     
     public Cargo(String inDest, double inTons)
     {
          setDest(inDest);
          setTonnage(inTons);
     }
     
     public String getDest()
     {
          return dest;
     }
     
     public double getTonnage()
     {
          return tons;
     }
     
     private void setDest(String inDest)
     {
          dest = inDest;
     }
     
     private void setTonnage(double inTons)
     {
          if (inTons >= 0)
          {
               tons = inTons;
          }
     }
     
}